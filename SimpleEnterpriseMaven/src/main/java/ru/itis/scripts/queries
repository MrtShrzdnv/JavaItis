select id_car
from test.cars, test.users
where users.residence = 'Kazan' and cars. = users.id_user;

select name
from test.users, test.cars
where cars.mileage > 100 and owners.owner_id = cars.owner_id;

select name, id_car
from test.users, test.cars
where cars.mileage > 50 and owners.age > 20 and owners.owner_id = cars.owner_id;